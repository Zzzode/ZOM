EBNF ::= { Production }
Production ::= Identifier '::=' Expression
Expression ::= Term { '|' Term }
Term ::= Factor { Factor }
Factor ::= Identifier | StringLiteral | '(' Expression ')'
Identifier ::= <letter> { <letter> | <digit> | '_' }
StringLiteral ::= '"' { <any character> } '"'
<letter> ::= 'a'..'z' | 'A'..'Z'
<digit> ::= '0'..'9'
<any character> ::= <letter> | <digit> | ' ' | '!' | '#' | '$' | '%' | '&' | '\'' | '(' | ')' | '*' | '+' | ',' | '-' | '.' | '/' | ':' | ';' | '<' | '=' | '>' | '?' | '@' | '[' | '\' | ']' | '^' | '_' | '{' | '|' | '}' | '~'
